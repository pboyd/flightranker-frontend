{
  "name": "flightranker-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chart.js": "^2.8.0",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.8.6"
  },
  "scripts": {
    "start": "parcel serve public/index.html",
    "build": "parcel build public/index.html",
